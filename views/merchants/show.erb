


<p>Viewing all transactions from</p>
<h1><%= @merchant.name %></h1>


<table>
  <tr>
    <th>id #</th>
    <th>Item</th>
    <th>Price</th>
    <th>Category</th>
    <th>Date</th>
  </tr>
  <% for batman in @transactions %>
  <tr>
    <td><a href="/transactions/<%= batman.id %>"><%= batman.id %></a></td>
    <td><%= batman.item %></td>
    <td><%= batman.price %></td>
    <td> <%=  batman.category.name %> </td>
    <td><%= batman.date %></td>
  </tr>


  <% end %>
</table>
